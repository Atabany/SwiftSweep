{
    "theme": {
        "primaryColor": "#7209B7",
        "secondaryColor": "#FFFFFF",
        "accentColor": "#F72585",
        "backgroundColor": "#000000",
        "textColor": "#FFFFFF",
        "highlightColor": "#4CC9F0",
        "darkMode": true
    },
    "typography": {
        "fontFamily": "'Inter', sans-serif",
        "fontSizes": {
            "h1": "48px",
            "h2": "32px",
            "body": "16px",
            "caption": "12px"
        },
        "fontWeights": {
            "regular": 400,
            "bold": 700
        }
    },
    "layout": {
        "grid": {
            "columns": 4,
            "gap": "24px"
        },
        "padding": "20px",
        "maxWidth": "1200px",
        "borderRadius": "12px"
    },
    "components": {
        "header": {
            "backgroundColor": "#000000",
            "textColor": "#FFFFFF",
            "logo": {
                "width": "50px",
                "height": "50px"
            }
        },
        "card": {
            "backgroundColor": "rgba(255, 255, 255, 0.1)",
            "textColor": "#FFFFFF",
            "borderRadius": "12px",
            "padding": "16px",
            "boxShadow": "0 4px 8px rgba(0, 0, 0, 0.2)"
        },
        "button": {
            "backgroundColor": "#7209B7",
            "textColor": "#FFFFFF",
            "borderRadius": "8px",
            "padding": "12px 24px",
            "hover": {
                "backgroundColor": "#560BAD"
            }
        },
        "gridChart": {
            "tileSize": "10px",
            "tileSpacing": "2px",
            "filledColor": "#4CC9F0",
            "emptyColor": "#333333"
        },
        "appStoreButton": {
            "backgroundColor": "#000000",
            "textColor": "#FFFFFF",
            "borderRadius": "12px",
            "padding": "12px",
            "iconSize": "24px"
        },
        "rating": {
            "starColor": "#FFD700",
            "textColor": "#FFFFFF"
        }
    },
    "breakpoints": {
        "mobile": "576px",
        "tablet": "768px",
        "desktop": "992px"
    }
}